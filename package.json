{
  "name": "link_converter",
  "version": "1.0.0",
  "description": "Test task",
  "main": "index.js",
  "dependencies": {
    "express": "^4.17.3",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "start": "./node_modules/.bin/nodemon index.js",
    "test": "jest --forceExit"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "500"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/index.js"
    ],
    "testURL": "http://localhost:3000/",
    "testMatch": [
      "**/tests/**/*.spec.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "Victor Depta",
  "license": "MIT"
}
